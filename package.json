{
  "name": "@earle-poole/scaffoldit",
  "version": "1.1.0",
  "main": "./dist/index",
  "description": "Scaffoldit is a CLI tool to scaffold React components and pages",
  "contributors": [
    {
      "name": "Earle Poole",
      "email": "eepoole4@gmail.com",
      "url": "https://github.com/Earle-Poole"
    }
  ],
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "bun-types": "latest"
  },
  "dependencies": {
    "bun": "1.0.4"
  },
  "peerDependencies": {
    "typescript": "5.2.2"
  },
  "scripts": {
    "dev": "bun --watch index.ts",
    "build": "bun build --minify index.ts --outfile=./dist/bundle.js",
    "scaffoldit": "bun run index.ts",
    "publish:npm": "npm publish --access public"
  },
  "bin": {
    "scaffoldit": "./dist/bundle.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Earle-Poole/scaffoldit.git"
  },
  "bugs": {
    "url": "https://github.com/Earle-Poole/scaffoldit/issues",
    "email": "eepoole4@gmail.com"
  }
}
